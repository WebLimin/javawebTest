<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>/</display-name>
	<welcome-file-list>
		<welcome-file>web/page/login.html</welcome-file>
		<welcome-file>web/page/login.jsp</welcome-file>
		<welcome-file>web/page/login.htm</welcome-file>
	</welcome-file-list>

	<!-- <filter-mapping> <filter-name>login</filter-name> <url-pattern>web/page/login</url-pattern> 
		</filter-mapping> -->

	<!-- <servlet-mapping> <servlet-name>default</servlet-name> <url-pattern>/*</url-pattern> 
		</servlet-mapping> -->

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>login.SrvLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

</web-app>